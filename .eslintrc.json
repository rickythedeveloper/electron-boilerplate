{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-typescript/base"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"import/no-extraneous-dependencies": ["off"],
		"no-plusplus": ["off"],
		"max-len": ["error", {"code": 120}]
	}
}
